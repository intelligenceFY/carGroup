.container6{
  display: flex;
  flex-direction: column;
  width: 650rpx;
  justify-content: flex-start;
  margin: 20rpx 0 90rpx 50rpx;

}
.display{
  margin-top: 40rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}
.total_display{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  background-color: #FF9800;
  text-align: center;
  line-height: 200rpx;
  color: #fff;
  font-weight: bold;
  font-size: 40rpx;
  cursor: pointer;
}
.success_display{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  background-color: #8BC34A;
  text-align: center;
  line-height: 200rpx;
  color: #fff;
  font-weight: bold;
  font-size: 40rpx;
  cursor: pointer;
}
.listContainer{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  border-top: 1px solid #ccc;
  font-size: 30rpx;
  margin-top: 80rpx;
}
.lable_box{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 14px;
  padding: 20rpx;
}
.list_box{
  height: 98rpx;
  align-items: center;
  font-size: 12px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  background-color: #fff;
  border-bottom: 1px solid #ededed;
  border-top: 1px solid #ededed;
  padding: 20rpx;
}
.start,.end{
  width: 240px;
  display: flex;
  flex-direction: row;
}