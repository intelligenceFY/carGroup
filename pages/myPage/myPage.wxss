.container2{
  display: flex;
  flex-direction: column;
  width: 650rpx;
  justify-content: flex-start;
  /* align-items: center; */
  margin: 0 0 90rpx 50rpx;
  border-bottom: 1px solid #ccc;
}
.icon_display{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.icon_display image{
  width: 500rpx;
  height: 400rpx;
}
.apply_container{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  color: #666;
  font-size: 30rpx;
}
.info,.be,.to_be,.success_list,.fail_list,.history_list{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding-left: 20rpx;
  padding-right: 20rpx;
  height: 100rpx;
  border-top: 1px solid #ccc;
  border-right: 1px solid #ccc;
  border-left: 1px solid #ccc;
  line-height: 100rpx;
}