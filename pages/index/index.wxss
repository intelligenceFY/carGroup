 .header {
    margin: 90rpx 0 90rpx 50rpx;
    text-align: center;
    width: 650rpx;
    height: 300rpx;
    line-height: 450rpx;
}
.header image {
    width: 100rpx;
    height: 100rpx;
} 
.content {
    margin-left: 50rpx;
    margin-bottom: 90rpx;
}
.content text {
    display: block;
    color: #9d9d9d;
    margin-top: 40rpx;
}
.bottom {
    border-radius: 80rpx;
    margin: 70rpx;
    font-size: 35rpx;
}
.mcontainer{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: flex-start;
  width: 650rpx;
  height: 150rpx;
  margin: 40rpx 0 0 50rpx; 
  border: 1rpx #666 solid;
}
.input_container{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  height: 100%;
}
.button_container{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  height: 100%;
}
.start_input{
  border: 1rpx #666 solid;
  height: 50rpx;
  width: 450rpx;
  font-size: 14px;
}
.choose{
  width: 120rpx;
  height: 120rpx;
  font-size: 24rpx;
  line-height: 120rpx;
}
.body_contanier{
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  flex-wrap: wrap;
  align-items: flex-start;
  width: 650rpx;
  height: 860rpx;
  font-size: 14px;
  margin: 40rpx 0 0 50rpx; 
  /* border: 1rpx #666 solid; */
}
.lable_box{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 650rpx;
  height: 70rpx;
  line-height: 70rpx;
  /* margin: 40rpx 0 0 50rpx;  */
  /* border-bottom: 1rpx #666 solid; */
}
.list_box{
  height: 98rpx;
  align-items: center;
  width: 650rpx;
  font-size: 12px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  background-color: #fff;
  border-bottom: 1px solid #ededed;
  border-top: 1px solid #ededed;
  padding-bottom: 5px;
}
.start,.end{
  width: 240px;
  display: flex;
  flex-direction: row;
}